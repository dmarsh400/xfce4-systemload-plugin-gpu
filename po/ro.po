#, c-format
msgid "%d hour"
msgid_plural "%d hours"
msgstr[0] "%d oră"
msgstr[1] "%d ore"
msgstr[2] "%d de ore"

#: panel-plugin/systemload.cc:284
#, c-format
msgid "%d minute"
msgid_plural "%d minutes"
msgstr[0] "%d minut"
